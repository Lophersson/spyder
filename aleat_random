1_CONSULTA ALEATORIA
import random
while True:
    aleat=random.randint(1, 1000)
    print("aleat=",aleat)    
    if aleat==500:
        break
print("fin")

2_NUEVA CONSULTA
import random
def f(x,y,z):
        while True:
            aleat = random.randint(x, y)
            print(aleat)
            if aleat == z:
                break
a = 5
b = 100
c = 80
f(a,b,c)


print("Fin del programa")
